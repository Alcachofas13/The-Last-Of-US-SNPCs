UnlitGeneric
{
$baseTexture "effects\tlou\flash_gen_side"
$basetexturetransform	"center .5 .5 scale 1 1 rotate -90  translate 0 0"
$translucent 1
$alpha 1
$additive 1
$vertexcolor	1

"Proxies"
   {
      "AnimatedTexture"
      {
         "animatedtexturevar" "$basetexture"
         "animatedtextureframenumvar" "$frame"
         "animatedtextureframerate" "30"
      }
   }
}
